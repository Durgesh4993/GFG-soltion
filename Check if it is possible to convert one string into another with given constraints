class Solution {


  public:

    int isItPossible(string S, string T, int M, int N) {

        // code here

          S.erase(remove(S.begin(), S.end(), '#'), S.end());

 

        T.erase(remove(T.begin(), T.end(), '#'), T.end());

 

        int equalOrNot = S.compare(T);

 

        if (equalOrNot == 0) {

 

            return 1;

 

        }

 

        return 0;

    }

};
